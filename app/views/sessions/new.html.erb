<div class="container">
  <h1> Log in </h1>
  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.text_field :email, placeholder: "Email" %>
    <%= f.password_field :password, placeholder: "Password" %>
    <%= f.submit "Log in" %>
  <% end %>
<p>New User? <%= link_to "Create an Account", new_user_path %></p>
</div>
